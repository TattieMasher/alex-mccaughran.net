/* Shared styles */
@import url("navbar.css");
@import url("index.css");
@import url("page-styles/about.css");

body {
    margin: 0px;
    padding-top: 90px;
    font-family: "Montserrat", sans-serif;
	font-weight: 400;
    font-size: 20px;
}

@keyframes fadeInAnimation {
	from {
		opacity: 0;
	}
	to {
		opacity: 1;
	}
}

@keyframes underlineAnimation {
	from {
		width: 0;
	}
	to {
		width: 100%;
	}
}

